void ActivationFunction(float *vec, int length,int act)
{
  if(act==0)
  {
   for (int j = 0; j < length; j++)
   {
    float v = vec[j];
    vec[j] = (2.0/(1 + exp(-2*v)))-1;
   }
    
  }
  else if(act==1)
{
  float sum=0;
   for (int j = 0; j < length; j++)
   {
    float v = vec[j];
    sum=sum+exp(v);
   }
    for (int j = 0; j < length; j++)
   {
    float v = vec[j];
    vec[j] = exp(v)/sum;
   }
  
    
}
  else if(act==2)
{
  
   for (int j = 0; j < length; j++)
   {
    float v = vec[j];
   if (v<0)
   {
    vec[j]=0;
   }
   
   }
   
  
    
}
}

void VectorMatrixMultiply(float* vecX, int xDim, float *wMat, int wCols, float* vecY,float *bias)
{
  int wRows = xDim;  //x is a vector, Order = (1 x xDim)
  int yDim = wCols; //vector y Dimension

  for (int dcounter = 0; dcounter < yDim; dcounter++) //destination layer
  { 
    float sum = 0.0;
    for (int scounter = 0; scounter < xDim; scounter++) //source layer
    {
      sum = sum+wMat[dcounter *  xDim + scounter] * vecX[scounter];  
             
    }
    sum=sum+bias[dcounter];
    vecY[dcounter] = sum; 
  }   
}

void MatrixPrint(float* A, int m, int n, String label)
{
  // A = input matrix (m x n)
  int i, j;
  Serial.println();
  Serial.println(label);
  for (i = 0; i < m; i++)
  {
    for (j = 0; j < n; j++)
    {
      Serial.print(A[n * i + j]);
      Serial.print("\t");
    }
    Serial.println();
  }
}
